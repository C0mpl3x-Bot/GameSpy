<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Complaint!</title>
  <link rel="stylesheet" type="text/css" href="/css/custom.css" />
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
  <script src="https://use.fontawesome.com/releases/v5.0.8/js/all.js"></script>

  <script src="js/js.js"></script>
  <script src="https://unpkg.com/scrollreveal"></script>
</head>

<body style="background-color:white">
  <h1> This is Complaint Page for Admin </h1>

  <ul>
    <div class="ptablegamepage" style="overflow-x:auto;">
      <table class="tablegamepage">
        <tr class="colgamepage">
          <th>_Id</th>
          <th>Email</th>
          <th>Subject</th>
          <th>Message</th>
        </tr>
        <% availablegame.forEach(function(game){ %>
          <tr class="wposgamepage">
            <td>
              <%= game._id %>
            </td>
            <td>
              <%= game.Email %>
            </td>
            <td>
              <%= game.Subject %>
            </td>
            <td>
              <%= game.Message %>
            </td>
          </tr>
          <% }) %>
      </table>
    </div>
  </ul>
  <div class="complaintsform">
  <form class="centered" method="POST" action="/deleted">
    <div class="form-group1">
      <label for="email">Email: </label>
      <input type="text" id="email" name="email" placeholder="email" required><br />
    </div>
    <button class="button forumbtn" type="submit">Submit</button>
  </form>
</div>
</body>

</html>
